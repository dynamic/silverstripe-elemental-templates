{
    "name": "dynamic/silverstripe-elemental-templates",
    "description": "Elemental Templates",
    "license": "BSD-3-Clause",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "element",
        "elemental",
        "content blocks",
        "templates"
    ],
    "authors": [
        {
            "name": "Jason Irish",
            "email": "dev@dynamicagency.com"
        },
        {
            "name": "Nic Horstmeier",
            "email": "dev@dynamicagency.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "dnadesign/silverstripe-elemental": "^6.0",
        "lekoala/silverstripe-cms-actions": "^2.0",
        "silverstripe/vendor-plugin": "^3"
    },
    "require-dev": {
        "silverstripe/recipe-testing": "^4",
        "phpstan/extension-installer": "^1.4",
        "cambis/silverstan": "^2.1",
        "squizlabs/php_codesniffer": "^4.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Dynamic\\ElementalTemplates\\": "src/",
            "Dynamic\\ElementalTemplates\\Tests\\": "tests/"
        }
    }
}
